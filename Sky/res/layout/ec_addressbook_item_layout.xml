<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:custom="http://schemas.android.com/apk/res/com.SkyPremiumLtd.SkyPremium"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:background="#ffffff"
     >
    <LinearLayout 
        android:layout_width="@dimen/dimen_300dp"
        android:layout_height="wrap_content"
        android:background="#ededed"
        android:layout_margin="@dimen/dimen_10dp"
        android:visibility="visible"
        android:orientation="vertical">
        
        <LinearLayout 
	        android:layout_width="@dimen/dimen_300dp"
	        android:layout_height="wrap_content"
	        android:background="#ededed"
	        android:layout_marginTop="@dimen/dimen_10dp"
	        android:orientation="horizontal">
	        	<org.com.atmarkcafe.sky.customviews.charting.MTextView 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="@string/ec_address_name"
		            custom:txtEng="@string/ec_address_name"
		            custom:txtJa="@string/ec_address_name_j"
		            style="@style/sky_ec_address_lable"
		            android:layout_marginLeft="@dimen/dimen_10dp"/>
	        	<org.com.atmarkcafe.sky.customviews.charting.MTextView 
	        	    android:id="@+id/ec_address_name_b"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            style="@style/sky_ec_address_value"/>
	    </LinearLayout>
	    <LinearLayout 
	        android:layout_width="@dimen/dimen_300dp"
	        android:layout_height="wrap_content"
	        android:background="#ededed"
	        android:layout_marginTop="@dimen/dimen_10dp"
	        android:orientation="horizontal">
	        	<org.com.atmarkcafe.sky.customviews.charting.MTextView 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="@string/ec_address_tel"
		            custom:txtEng="@string/ec_address_tel"
		            custom:txtJa="@string/ec_address_tel_j"
		            style="@style/sky_ec_address_lable"
		            android:layout_marginLeft="@dimen/dimen_10dp"/>
	        	<org.com.atmarkcafe.sky.customviews.charting.MTextView 
	        	    android:id="@+id/ec_address_tel_b"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            style="@style/sky_ec_address_value"/>
	    </LinearLayout>
	    <LinearLayout 
	        android:layout_width="@dimen/dimen_300dp"
	        android:layout_height="wrap_content"
	        android:background="#ededed"
	        android:layout_marginTop="@dimen/dimen_10dp"
	        android:orientation="horizontal">
	        	<org.com.atmarkcafe.sky.customviews.charting.MTextView 
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            android:text="@string/ec_address_shipping"
		            custom:txtEng="@string/ec_address_shipping"
		            custom:txtJa="@string/ec_address_shipping_j"
		            style="@style/sky_ec_address_lable"
		            android:layout_marginLeft="@dimen/dimen_10dp"/>
	        	<org.com.atmarkcafe.sky.customviews.charting.MTextView 
	        	    android:id="@+id/ec_address_shipping_b"
		            android:layout_width="wrap_content"
		            android:layout_height="wrap_content"
		            style="@style/sky_ec_address_value"/>
	    </LinearLayout>
	     <LinearLayout
	        android:layout_width="wrap_content"
	        android:layout_height="match_parent"
	        android:background="@drawable/button_ec_addressnew_radius"
	        android:orientation="horizontal"
	        android:gravity="center_horizontal|center_vertical"
	        android:layout_marginTop="@dimen/dimen_10dp"
	        android:layout_marginLeft="@dimen/dimen_10dp"
	        android:layout_marginBottom="@dimen/dimen_10dp">
	         <ImageView 
	             android:layout_width="@dimen/dimen_16dp"
	             android:layout_height="@dimen/dimen_16dp"
	             android:layout_gravity="center_vertical"
	             android:layout_marginLeft="@dimen/dimen_10dp"
	             android:src="@drawable/ec_addbook_select"/>
	         <org.com.atmarkcafe.sky.customviews.charting.MTextView
                 android:id="@+id/bt_add_new"
                 android:layout_width="wrap_content"
                 android:layout_height="@dimen/dimen_40dp"
                 android:paddingLeft="@dimen/dimen_10dp"
                 android:paddingRight="@dimen/dimen_10dp"
                 android:gravity="center_vertical|left"
                 android:text="@string/ec_address_book_select"
                 android:textColor="#ffffff"
                 android:textSize="@dimen/dimen_12dp"
                 custom:txtEng="@string/ec_address_book_select"
                 custom:txtJa="@string/ec_address_book_select_j"/>
         </LinearLayout>
	    
    </LinearLayout>

</RelativeLayout>
